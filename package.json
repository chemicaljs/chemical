{
  "name": "chemicaljs",
  "version": "2.6.4",
  "description": "Easily create your own web proxy with no experience required.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "client",
    "config",
    "ultraviolet"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chemicaljs/chemical.git"
  },
  "author": "Nebelung",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "npm run build-uv && tsup",
    "prepack": "pnpm build",
    "build-uv": "node build-uv.js",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@mercuryworkshop/bare-mux": "^2.1.7",
    "@mercuryworkshop/epoxy-transport": "^2.1.28",
    "@mercuryworkshop/libcurl-transport": "^1.5.0",
    "@mercuryworkshop/scramjet": "github:MercuryWorkshop/scramjet",
    "@mercuryworkshop/wisp-js": "^0.3.3",
    "@titaniumnetwork-dev/ultraviolet": "^3.2.10",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "nanoid": "^5.1.5",
    "rammerhead": "file:rammerhead.tgz",
    "typescript": "^5.9.2",
    "vite": "^7.1.4"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
